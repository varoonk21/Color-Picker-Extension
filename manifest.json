{
    "manifest_version": 3,
    "name": "Color Picker",
    "version": "1.0",
    "description": "A basic color picker extension which help in getting the desired colors from web.",
    "action": {
        "default_popup": "popup/popup.html"
    },
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "64": "images/icon-64.png",
        "128": "images/icon-128.png"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "permissions": ["storage"]
        
    
}